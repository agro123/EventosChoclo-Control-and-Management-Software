{"ast":null,"code":"import data from './data.json';\nvar eventos = data.eventos;\nexport var getEventos = function getEventos() {\n  return new Promise(function (resolve, reject) {\n    setTimeout(function () {\n      if (eventos) {\n        resolve(eventos);\n      } else {\n        reject('No hay eventos');\n      }\n    }, 1000);\n  });\n};\nexport var deleteEvento = function deleteEvento(id) {\n  return new Promise(function (resolve, reject) {\n    var found = false;\n    setTimeout(function () {\n      eventos.map(function (v, i) {\n        if (v.id === id) {\n          found = true;\n          eventos.splice(i, 1);\n        }\n      });\n      found ? resolve(true) : reject(false);\n    }, 1500);\n  });\n};\nexport var getEventoById = function getEventoById(id) {\n  return new Promise(function (resolve, reject) {\n    setTimeout(function () {\n      var evento = eventos.find(function (evento) {\n        return evento.id === id;\n      });\n      evento ? resolve(evento) : reject('Not found');\n    }, 1000);\n  });\n};","map":{"version":3,"sources":["C:/Users/crist/Documents/ds2/EventosChoclo-Control-and-Management-Software/components/database/eventosCrud.js"],"names":["data","eventos","getEventos","Promise","resolve","reject","setTimeout","deleteEvento","id","found","map","v","i","splice","getEventoById","evento","find"],"mappings":"AAAA,OAAOA,IAAP,MAAiB,aAAjB;IACQC,O,GAAYD,I,CAAZC,O;AAER,OAAO,IAAMC,UAAU,GAAG,SAAbA,UAAa,GAAM;AAC5B,SAAO,IAAIC,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACpCC,IAAAA,UAAU,CAAC,YAAM;AACb,UAAIL,OAAJ,EAAa;AACTG,QAAAA,OAAO,CAACH,OAAD,CAAP;AACH,OAFD,MAEO;AACHI,QAAAA,MAAM,CAAC,gBAAD,CAAN;AACH;AACJ,KANS,EAMP,IANO,CAAV;AAOH,GARM,CAAP;AASH,CAVM;AAYP,OAAO,IAAME,YAAY,GAAG,SAAfA,YAAe,CAAAC,EAAE,EAAI;AAC9B,SAAO,IAAIL,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACpC,QAAII,KAAK,GAAG,KAAZ;AACAH,IAAAA,UAAU,CAAC,YAAM;AACbL,MAAAA,OAAO,CAACS,GAAR,CAAY,UAACC,CAAD,EAAIC,CAAJ,EAAU;AAClB,YAAID,CAAC,CAACH,EAAF,KAASA,EAAb,EAAiB;AACbC,UAAAA,KAAK,GAAG,IAAR;AACAR,UAAAA